{
  "name": "wordpress-gatsby-migrator",
  "private": true,
  "version": "1.0.6",
  "description": "Migrate your WordPress blog to Gatsby. This tool takes a Wordpress export, and produces a local folder structure with the content and images.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "debug": "node --nolazy --inspect-brk=9229 src/index.js input.xml blog"
  },
  "bin": {
    "wordpress-gatsby-migrator": "src/index.js"
  },
  "author": {
    "name": "Sam Leatherdale (forked from Weiran Zhang)",
    "email": "weiran@zhang.me.uk",
    "url": "https://weiran.co"
  },
  "homepage": "https://github.com/SamLeatherdale/wordpress-gatsby-migrator",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "gatsby",
    "markdown",
    "squarespace"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "davefeedread": "^0.5.9",
    "fs-extra": "^8.0.1",
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.0",
    "sanitize-filename": "^1.6.3",
    "turndown": "^6.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/fs-extra": "^9.0.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.0.13",
    "@types/turndown": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
